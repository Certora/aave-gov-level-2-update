{
  "name": "aave-gov-level-2-proposal",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "test:contracts": "forge test --fork-url $FORK_URL -vvvv",
    "aaveEcosystemReserveV2:current": "env-cmd cast etherscan-source -d etherscan 0x1aa435ed226014407Fa6b889e9d06c02B1a12AF3",
    "aaveEcosystemReserveV2:diff": "npm run aaveEcosystemReserveV2:current && sh generateDiffs.sh ./etherscan/AaveEcosystemReserveV2/AaveEcosystemReserveV2.sol ./src/contracts/AaveEcosystemReserveV2.sol AaveEcosystemReserveV2",
    "longExecutor:current": "env-cmd cast etherscan-source -d etherscan 0x61910EcD7e8e942136CE7Fe7943f956cea1CC2f7",
    "longExecutor:diff": "npm run longExecutor:current && sh generateDiffs.sh ./etherscan/Executor/Executor.sol ./src/contracts/LongExecutor.sol LongExecutor",
    "deployTest:le": "env-cmd npm run deploy:test:le",
    "deployTest:erV2": "env-cmd npm run deploy:test:erV2",
    "deploy:le": "env-cmd npm run deploy:broadcast:le",
    "deploy:erV2": "env-cmd npm run deploy:broadcast:erV2",
    "deployVerify:le": "env-cmd npm run deploy:verify:le",
    "deployVerify:erv2": "env-cmd npm run deploy:verify:reV2",
    "deploy:test:le": "forge script ./src/deploy/deployLongExecutor.sol --fork-url $FORK_URL --private-key $PRIVATE_KEY --optimize --optimizer-runs 200 -vvvv",
    "deploy:test:erV2": "forge script ./src/deploy/deployEcosystemReserve.sol --fork-url $FORK_URL --private-key $PRIVATE_KEY --optimize --optimizer-runs 200 -vvvv",
    "deploy:broadcast:le": "forge script ./src/deploy/deployLongExecutor.sol --private-key $PRIVATE_KEY --broadcast --fork-url $FORK_URL --verify --optimize --optimizer-runs 200 -vvvv",
    "deploy:verify:le": "forge script ./src/deploy/deployLongExecutor.sol --private-key $PRIVATE_KEY --fork-url $FORK_URL --verify --optimize --optimizer-runs 200 -vvvv",
    "deploy:broadcast:erV2": "forge script ./src/deploy/deployEcosystemReserve.sol --private-key $PRIVATE_KEY --broadcast --fork-url $FORK_URL --verify --optimize --optimizer-runs 200 -vvvv",
    "deploy:verify:erV2": "forge script ./src/deploy/deployEcosystemReserve.sol --private-key $PRIVATE_KEY --fork-url $FORK_URL --verify --optimize --optimizer-runs 200 -vvvv"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bgd-labs/aave-gov-level-2-update.git"
  },
  "keywords": [],
  "author": "sendra",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bgd-labs/aave-gov-level-2-update/issues"
  },
  "homepage": "https://github.com/bgd-labs/aave-gov-level-2-update#readme",
  "devDependencies": {
    "env-cmd": "^10.1.0"
  }
}
